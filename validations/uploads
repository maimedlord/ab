{
  $jsonSchema: {
    bsonType: 'object',
    required: [
      'contractid',
      'data',
      'date',
      'downloader',
      'uploader'
    ],
    properties: {
      contractid: {
        bsonType: 'objectId',
        description: 'uploads: contractid'
      },
      data: {
        bsonType: 'binData',
        description: 'uploads: data'
      },
      date: {
        bsonType: 'date',
        description: 'uploads: date'
      },
      downloader: {
        bsonType: [
          'null',
          'objectId'
        ],
        description: 'uploads: downloader'
      },
      uploader: {
        bsonType: 'objectId',
        description: 'uploads: uploader'
      }
    }
  }
}